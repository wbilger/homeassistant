                            elements:
                              - type: image
                                image: /local/photos/light_outside.png
                                entity: light.entrance_light_porch
                                tap_action: 
                                  action: toggle
                                state_filter:
                                  "off": opacity(33%)
                                  "unavailable": opacity(20%) brightness(10%)
                                style:
                                  width: 50%
                                  top: 50%
                                  left: 50%
                                  
                      - type: custom:tiles-card-deleteme
                        card_settings:
                          columns: 3
                          row_height: 35px
                          padding: 0px
                          gap: 0px
                        global_settings:
                          background:
                            value: gray
                            value_on: yellow
                          label:
                            transform: capitalize
                            color: black
                            size: 17px
                          templates:
                            style: "return 'font-weight: bold;'"
                        entities:
                          - entity: lock.schlage_allegion_be469_touchscreen_deadbolt_locked_frontdoor
                            row: 1
                            column: 1
                            templates:
                              label: return state;
                            background: 
