#   Middle Section - column 1 (of 3)
################################################################# 
          - type: custom:layout-card
            layout: horizontal
            cards:
              - type: vertical-stack
                cards:
                  - type: custom:vertical-stack-in-card
                    cards:
                      - type: horizontal-stack
                        cards:
                          - type: picture-elements
                            image: /local/photos/gray-light.png
                            aspect_ratio: 66%
                            elements:
                              - type: image
                                image: /local/photos/lock_door_unlocked.png
                                entity: lock.schlage_allegion_be469_touchscreen_deadbolt_locked_frontdoor
                                tap_action: 
                                  action: toggle
                                hold_action:
                                  action: more-info
                                state_image:  
                                  "locked": /local/photos/lock_door_locked.png
                                state_filter:
                                  "unlocked": opacity(33%)
                                  "unavailable": opacity(20%) brightness(10%)
                                style:
                                  width: 50%
                                  top: 50%
                                  left: 50%
                          - type: picture-elements
                            image: /local/photos/gray-light.png
                            aspect_ratio: 66%
                            elements:
                              - type: image
                                image: /local/photos/light_ceiling.png
                                entity: light.entrance_light
