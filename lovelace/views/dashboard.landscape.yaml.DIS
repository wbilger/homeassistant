                                  action: toggle
                                state_filter:
                                  "off": opacity(33%)
                                  "unavailable": opacity(20%) brightness(10%)
                                style:
                                  width: 60%
                                  top: 50%
                                  left: 50%
                          - type: picture-elements
                            image: /local/photos/gray-light.png
