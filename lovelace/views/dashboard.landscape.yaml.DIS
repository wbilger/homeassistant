#################################################################
# VIEW_home                                                     
#   Header banner - bottom info - weather summaries 
################################################################# 
          - type: picture-elements
            image: /local/photos/transparent.png
            aspect_ratio: 2%
            elements: 
              - type: state-label
                entity: input_text.dark_sky_summaries   #rotating minutely/hourly/daily summaries
                tap_action:
                  action: navigate
                  navigation_path: /lovelace/weather
                style:
                  font-size: 20px
                  font-weight: bold
                  color: var(--secondary-text-color)
                  top: 50%
                  left: 50%

#################################################################
